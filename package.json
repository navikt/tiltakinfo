{
  "name": "tiltakinfo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build:ts": "react-scripts build",
    "deploy": "npm run build && gh-pages -d build",
    "test": "react-scripts test --env=jsdom",
    "lint": "tslint ./src/**/*.ts*",
    "lint:fix": "tslint ./src/**/*.ts* --fix",
    "test:coverage": "npm test -- --coverage",
    "prettier": "prettier --write --single-quote --tab-width 4 src/**/*.ts src/**/*.tsx src/**/*.less",
    "eject": "react-scripts eject",
    "start-mock": "cross-env REACT_APP_MOCK_FULL=true npm-run-all -p start:*",
    "start-mock:halv": "cross-env REACT_APP_MOCK_HALV=true npm-run-all -p start:*",
    "build:tekster": "node ./src/utils/build-tekster.js",
    "build": "npm run build:ts && npm run build:tekster"
  },
  "dependencies": {
    "@nutgaard/react-scripts": "0.0.7",
    "nav-frontend-core": "4.0.5",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "@types/jest": "23.0.0",
    "@types/node": "10.3.1",
    "@types/react": "16.3.16",
    "@types/react-dom": "16.0.6",
    "@types/react-redux": "6.0.2",
    "@types/react-router-dom": "4.2.7",
    "typescript": "2.9.1"
  }
}
